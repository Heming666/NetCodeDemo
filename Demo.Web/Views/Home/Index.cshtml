<div class="row-fluid">
    <!-- block -->
    <div class="block">
        <div class="navbar navbar-inner block-header">
            <div class="muted pull-left">统计</div>
            <div class="pull-right">
                <span class="badge badge-success"><a asp-action="Index" asp-controller="Comsume" asp-area="Customer" asp-route-UserId="1" style="text-decoration:none;color:white;">消费详情</a></span>
            </div>
        </div>
        <div class="block-content collapse in">
            <div class="row-fluid">
                <select class="form-control-sm pull-right" id="month" onchange="LoadPie()">
                    @Html.Raw(htmlStr)
                </select>
            </div>

            <div class="row-fluid">
                <div class="span6">
                    <div id="pie"></div>
                </div>
                <div class="span6">
                    <div id="YearPie"></div>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span12">
                    <select class="form-control-sm pull-right" id="columnYear" onchange="loadColumn()">
                        @{ int year = DateTime.Now.Year, count = 0;}
                        @while (count <= 5)
                        {
                            year -= count;
                            string selectStr = year == DateTime.Now.Year ? "selected=\"selected\"" : "";
                            @Html.Raw($"<option value=\"{ year}\" {selectStr} >{year} 年</option>")count++;
                        }
                    </select>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span12">
                    <div id="column"></div>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span12">
                    <div id="line"></div>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span12">
                    <select class="form-control-sm pull-right" id="columnMonth" onchange="LoadMonthColumn()">
                        @Html.Raw(htmlStr)
                    </select>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span12">
                    <div id="monthLine"></div>
                </div>
            </div>
        </div>
    </div>
    <!-- /block -->
</div>
