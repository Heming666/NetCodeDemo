@{
    int month = 1;
    string htmlStr = string.Empty;
    while (month <= 12)
    {
        string selectStr = month == DateTime.Now.Month ? "selected=\"selected\"" : "";
        htmlStr += $"<option value=\"{ month}\" {selectStr} >{month} 月</option>";
        month++;
    }
}
@section scripts
{
    <script src="~/lib/highcharts/highcharts.js"></script>
}
@section styles
{
    <link href="~/lib/highcharts/css/highcharts.min.css" rel="stylesheet" />
}

<script>
    $(function () {
        LoadPie();
        LoadYearPie();
        loadColumn();
        LoadMonthColumn();
    })
    var LoadPie = () => {
        var month = $("#month").val();
        $.ajax({
            url: "@Url.Action("LoadPie","Comsume",new { area="Customer"})",
            data: { month: month },
            type: "POST",
            success: (data) => {
                if (!!data && data.length == 0) {
                    Highcharts.setOptions({
                        lang: {
                            noData: '暂无数据'
                        }
                    });
                }
                /**加载饼图**/
                 Highcharts.chart('pie', {
                    chart: {
                        plotBackgroundColor: null,
                        plotBorderWidth: null,
                        plotShadow: false,
                        type: 'pie',
                    },
                    title: {
                        text: '本月各项消费占比'
                    },
                    tooltip: {
                        pointFormat: '{series.name}: <b>{point.percentage:.1f}%</b>'
                    },
                    plotOptions: {
                        pie: {
                            allowPointSelect: true,
                            cursor: 'pointer',
                            dataLabels: {
                                enabled: true,
                                format: '<b>{point.name}</b>: {point.percentage:.1f} %',
                                style: {
                                    color: (Highcharts.theme && Highcharts.theme.contrastTextColor) || 'black'
                                }
                            }
                        }
                    },
                    series: [{
                        name: '消费类别',
                        colorByPoint: true,
                        data: data
                    }]
                });
            },
            error: (error) => {
                console.error(error);
            }
        })
    }
    var LoadYearPie = () => {
        $.ajax({
            url:"@Url.Action("LoadYearPie", "Comsume", new { area = "Customer" })",
            type: "POST",
            success: (data) => {

                /**加载饼图**/
                 Highcharts.chart('YearPie', {
                    chart: {
                        plotBackgroundColor: null,
                        plotBorderWidth: null,
                        plotShadow: false,
                        type: 'pie',
                    },
                    title: {
                        text: '年度各项消费占比'
                    },
                    tooltip: {
                        pointFormat: '{series.name}: <b>{point.percentage:.1f}%</b>'
                    },
                    plotOptions: {
                        pie: {
                            allowPointSelect: true,
                            cursor: 'pointer',
                            dataLabels: {
                                enabled: true,
                                format: '<b>{point.name}</b>: {point.percentage:.1f} %',
                                style: {
                                    color: (Highcharts.theme && Highcharts.theme.contrastTextColor) || 'black'
                                }
                            }
                        }
                    },
                    series: [{
                        name: '消费类别',
                        colorByPoint: true,
                        data: data
                    }]
                });
            },
            error: (error) => {
                console.error(error);
            }
        })
    }
    var loadColumn = () => {
        var year = $("#columnYear").val()
          $.ajax({
            url: "@Url.Action("LoadColumn", "Comsume", new { area = "Customer" })",
              data: { year: year },
            type: "POST",
            success: (data) => {
                Highcharts.chart('column', {
                    chart: {
                        type: 'column'
                    },
                    title: {
                        text: '年度消费情况'
                    },
                    subtitle: {
                        text: '每月各项消费详情'
                    },
                    xAxis: {
                        categories: [
                            '一月', '二月', '三月', '四月', '五月', '六月', '七月', '八月', '九月', '十月', '十一月', '十二月'
                        ],
                        crosshair: true
                    },
                    yAxis: {
                        min: 0,
                        title: {
                            text: '消费的金额（元）'
                        }
                    },
                    tooltip: {
                        // head + 每个 point + footer 拼接成完整的 table
                        headerFormat: '<span style="font-size:10px">{point.key}</span><table>',
                        pointFormat: '<tr><td style="color:{series.color};padding:0">{series.name}: </td>' +
                            '<td style="padding:0"><b>{point.y:.1f} 元</b></td></tr>',
                        footerFormat: '</table>',
                        shared: true,
                        useHTML: true
                    },
                    plotOptions: {
                        column: {
                            borderWidth: 0
                        }
                    },
                    series: data
                });

                var chart = Highcharts.chart('line', {
                    chart: {
                        type: 'line'
                    },
                    title: {
                        text: '年度消费趋势'
                    },
                    subtitle: {
                        text: '各类支出情况'
                    },
                    xAxis: {
                        categories: ['一月', '二月', '三月', '四月', '五月', '六月', '七月', '八月', '九月', '十月', '十一月', '十二月']
                    },
                    yAxis: {
                        title: {
                            text: '金额 (￥)'
                        }
                    },
                    plotOptions: {
                        line: {
                            dataLabels: {
                                // 开启数据标签
                                enabled: true
                            },
                            // 关闭鼠标跟踪，对应的提示框、点击事件会失效
                            enableMouseTracking: true
                        }
                    },
                    series: data
                });
            },
            error: (error) => {
                console.error(error);
            }
        })

    }

    var LoadMonthColumn = () => {
        var month = $("#columnMonth").val()
          $.ajax({
              url: "@Url.Action("LoadMonthColumn", "Comsume", new { area = "Customer" })",
              data: { month: month },
            type: "POST",
            success: (data) => {

                var chart = Highcharts.chart('monthLine', {
                    chart: {
                        type: 'line'
                    },
                    title: {
                        text: '月度消费趋势'
                    },
                    subtitle: {
                        text: '各类支出情况'
                    },
                    xAxis: {
                        categories: data.days
                    },
                    yAxis: {
                        title: {
                            text: '金额 (￥)'
                        }
                    },
                    plotOptions: {
                        line: {
                            dataLabels: {
                                // 开启数据标签
                                enabled: true
                            },
                            // 关闭鼠标跟踪，对应的提示框、点击事件会失效
                            enableMouseTracking: true
                        }
                    },
                    series: data.data
                });
            },
            error: (error) => {
                console.error(error);
            }
        })
    }
</script>
