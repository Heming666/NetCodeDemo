
<aside class="main-sidebar" ng-app="B2BWebMenu">

    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">

        <!-- Sidebar user panel (optional) -->
        <div class="user-panel">
            <div class="pull-left image">
                <img src="~/plugins/AdminLTE/dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">
            </div>
            <div class="pull-left info">
                <p id="MenuUserName">管理员</p>
                <!-- Status -->
                <a href="#"><i class="fa fa-circle text-success"></i> 在线</a>
            </div>
        </div>

        <!-- Sidebar Menu -->
        <ul class="sidebar-menu" ng-controller="MenuCtrl">
            <li class="header"></li>
            <li class="treeview" ng-repeat="sMenu in Menu">
                <a href="javascript:;" ng-click="fnMenuClick($event,sMenu)">
                    <i class="{{sMenu.MenuIcon}}"></i> <span>{{sMenu.MenuName}}</span>
                    <span class="pull-right-container" ng-if="sMenu.SubMenu.length > 0">
                        <i class="fa fa-angle-left pull-right"></i>
                    </span>
                </a>
                <ul id="menuChildren" class="treeview-menu" ng-if="sMenu.SubMenu.length > 0">
                    <li ng-repeat="subMenu in sMenu.SubMenu">
                        <a id="childrenList" href="javascript:;" ng-click="fnMenuClick($event,subMenu)">
                            <i class="{{subMenu.MenuIcon}}"></i> <span id="childrenSpan">{{subMenu.MenuName}}</span>
                        </a>
                    </li>
                </ul>
            </li>
        </ul>
        <!-- /.sidebar-menu -->
    </section>
    <!-- /.sidebar -->
</aside>
<script src="~/Scripts/AdminLayout/Menu.js"></script>